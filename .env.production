# Production Environment Variables
NODE_ENV=production
VITE_API_URL=https://api.stayfit.pk
VITE_APP_NAME=StayFit
VITE_APP_VERSION=1.0.0
VITE_DEBUG=false
VITE_LOG_LEVEL=error

# Database (for reference - backend uses these)
DATABASE_URL=postgresql://username:password@localhost:5432/stayfit_prod
JWT_SECRET=your-super-secure-production-jwt-secret-key
JWT_EXPIRES_IN=24h

# CORS Settings
VITE_CORS_ORIGIN=https://stayfit.pk

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_TOOLS=false
VITE_ENABLE_HOT_RELOAD=false

# Security
VITE_HTTPS_ONLY=true
VITE_SECURE_COOKIES=true

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
